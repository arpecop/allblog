0441\\u0440\\u0430\\u0432\\u043D\\u0435\\u043D\\u0438\\u0435 \\u0441 \\u043D\\u0435\\u0447\\u0438\\u0439 \\u0434\\u0440\\u0443\\u0433. \\u0422\\u0435 \\u0441\\u0430 \\u0441\\u0435 \\u043F\\u0440\\u043E\\u0432\\u0430\\u043B\\u0438\\u043B\\u0438 \\u0432 \\u0436\\u0438\\u0432\\u043E\\u0442\\u0430. \\u0422\\u0435 \\u043E\\u043A\\u0430\\u0437\\u0432\\u0430\\u0442 \\u0443\\u0436\\u0430\\u0441\\u043D\\u043E \\u043C\\u043D\\u043E\\u0433\\u043E \\u043D\\u0430\\u0442\\u0438\\u0441\\u043A \\u0432\\u044A\\u0440\\u0445\\u0443 \\u0441\\u0435\\u0431\\u0435 \\u0441\\u0438 \\u043D\\u0435\\u043D\\u0443\\u0436\\u043D\\u043E \\u0438 \\u0432 \\u0435\\u0434\\u0438\\u043D \\u043C\\u043E\\u043C\\u0435\\u043D\\u0442 \\u0442\\u0435 \\u043F\\u0440\\u043E\\u0441\\u0442\\u043E \\u0449\\u0435 \\u0438\\u0437\\u0433\\u043E\\u0440\\u044F\\u0442 - \\u043A\\u0430\\u043A\\u0442\\u043E \\u0444\\u0438\\u0437\\u0438\\u0447\\u0435\\u0441\\u043A\\u0438, \\u0442\\u0430\\u043A\\u0430 \\u0438 \\u0435\\u043C\\u043E\\u0446\\u0438\\u043E\\u043D\\u0430\\u043B\\u043D\\u043E.\\u041E\\u043D\\u0435\\u0437\\u0438, \\u043A\\u043E\\u0438\\u0442\\u043E \\u0441\\u0430 \\u0435\\u043A\\u0441\\u043F\\u0430\\u043D\\u0437\\u0438\\u0432\\u043D\\u0438 \\u043F\\u0440\\u0438\\u0441\\u043F\\u043E\\u0441\\u043E\\u0431\\u044F\\u0432\\u0430\\u0442 \\u043A\\u044A\\u043C \\u0435\\u0434\\u043D\\u0438 \\u0438 \\u0441\\u044A\\u0449\\u0438 \\u0441\\u0438\\u0442\\u0443\\u0430\\u0446\\u0438\\u0438 \\u043A\\u0430\\u0442\\u043E \\u0432\\u044A\\u0437\\u043C\\u043E\\u0436\\u043D\\u043E\\u0441\\u0442 \\u0434\\u0430 \\u0443\\u0447\\u0430\\u0442 \\u0438 \\u0434\\u0430 \\u0440\\u0430\\u0441\\u0442\\u0430\\u0442. \\u0422\\u0435\\u0445\\u043D\\u0438\\u0442\\u0435 \\u0443\\u043C\\u043E\\u0432\\u0435 \\u0441\\u0435 \\u0440\\u0430\\u0437\\u0448\\u0438\\u0440\\u044F\\u0432\\u0430\\u0442, \\u0434\\u043E\\u043A\\u0430\\u0442\\u043E \\u0442\\u044A\\u0440\\u0441\\u044F\\u0442 \\u0442\\u0432\\u043E\\u0440\\u0447\\u0435\\u0441\\u043A\\u0438 \\u0440\\u0435\\u0448