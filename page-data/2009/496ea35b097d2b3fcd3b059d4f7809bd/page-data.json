043D \\u0436\\u0438\\u0432\\u043E\\u0442. \\u0412\\u0440\\u0435\\u043C\\u0435\\u0442\\u043E \\u0438\\u0433\\u0440\\u0430\\u0435 \\u0440\\u0435\\u0448\\u0430\\u0432\\u0430\\u0449\\u0430 \\u0440\\u043E\\u043B\\u044F \\u0432 \\u043D\\u0430\\u0448\\u0438\\u044F \\u043F\\u0440\\u043E\\u0444\\u0435\\u0441\\u0438\\u043E\\u043D\\u0430\\u043B\\u0435\\u043D \\u0438 \\u043B\\u0438\\u0447\\u0435\\u043D \\u0436\\u0438\\u0432\\u043E\\u0442. \\u0415\\u0442\\u043E \\u0437\\u0430\\u0449\\u043E \\u0441\\u0442\\u0430\\u0432\\u0430 \\u043E\\u0449\\u0435 \\u043F\\u043E-\\u0432\\u0430\\u0436\\u043D\\u043E \\u0434\\u0430 \\u0437\\u043D\\u0430\\u0435\\u0442\\u0435 \\u043A\\u0430\\u043A \\u0434\\u0430 \\u0433\\u043E \\u0443\\u043F\\u0440\\u0430\\u0432\\u043B\\u044F\\u0432\\u0430\\u0442\\u0435 \\u0435\\u0444\\u0435\\u043A\\u0442\\u0438\\u0432\\u043D\\u043E. \\u0415\\u0442\\u043E \\u043D\\u044F\\u043A\\u043E\\u043B\\u043A\\u043E \\u0441\\u044A\\u0432\\u0435\\u0442\\u0430 \\u043E\\u0442\\u043D\\u043E\\u0441\\u043D\\u043E \\u0443\\u043F\\u0440\\u0430\\u0432\\u043B\\u0435\\u043D\\u0438\\u0435\\u0442\\u043E \\u043D\\u0430 \\u0432\\u0440\\u0435\\u043C\\u0435\\u0442\\u043E: 1. \\u041F\\u043E\\u0441\\u0442\\u0430\\u0432\\u0435\\u0442\\u0435 SMART \\u0446\\u0435\\u043B\\u0438 - \\u0412\\u0438\\u043D\\u0430\\u0433\\u0438 \\u0437\\u0430\\u0434\\u0430\\u0432\\u0430\\u0439\\u0442\\u0435 \\u043A\\u043E\\u043D\\u043A\\u0440\\u0435\\u0442\\u043D\\u0438, \\u0438\\u0437\\u043C\\u0435\\u0440\\u0438\\u043C\\u0438, \\u0434\\u043E\\u0441\\u0442\\u0438\\u0436\\u0438\\u043C\\u0438, \\u0432\\u044A\\u0437\\u043D\\u0430\\u0433\\u0440\\u0430\\u0436\\u0434\\u0430\\u0432\\u0430\\u0449\\u0438 \\u0438 \\u043D\\u0430\\u0432\\u0440\\u0435\\u043C\\u0435\\u043D\\u043D\\u0438 \\u0446\\u0435\\u043B\\u0438, \\u0442.\\u0435. SMART \\u0446\\u0435\\u043B\\u0438.2. \\u0417\\u043D\\u0430\\u0439\\u0442\\u0435 \\u0441\\u0432\\u043E\\u0438\\u0442\\u0435 \\u043F\\u0440\\u0438\\u043E\\u0440\\u0438\\u0442\\u0435\\u0442\\u0438 - \\u0410\\u04