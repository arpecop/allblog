7\\u0438 \\u0441\\u043B\\u0435\\u0434 \\u0440\\u0430\\u0437\\u0434\\u044F\\u043B\\u0430 \\u0438\\u043F\\u0440\\u0435\\u0441\\u0442\\u0430\\u043D\\u0435\\u0442\\u0435 \\u0434\\u0430 \\u043E\\u0431\\u0438\\u0447\\u0430\\u0442\\u0435 \\u043F\\u0430\\u0440\\u0442\\u043D\\u044C\\u043E\\u0440\\u0430 \\u0441\\u0438. \\u041B\\u044E\\u0431\\u043E\\u0432\\u0442\\u0430 \\u043D\\u0435 \\u0441\\u0435 \\u0437\\u0430\\u0431\\u0440\\u0430\\u0432\\u044F \\u043F\\u043E\\u0447\\u0442\\u0438\\u0432\\u0435\\u0434\\u043D\\u0430\\u0433\\u0430! \\u0422\\u0430\\u0437\\u0438 \\u0441\\u0442\\u0430\\u0442\\u0438\\u044F \\u0449\\u0435 \\u0432\\u0438 \\u0440\\u0430\\u0437\\u043A\\u0440\\u0438\\u0435 \\u043A\\u0430\\u043A \\u0434\\u0430 \\u0440\\u0430\\u0437\\u0431\\u0435\\u0440\\u0435\\u0442\\u0435 \\u0434\\u0430\\u043B\\u0438 \\u0431\\u0438\\u0432\\u0448\\u0438\\u044F\\u0442 \\u0432\\u0438 \\u0432\\u0441\\u0435 \\u043E\\u0449\\u0435 \\u043E\\u0431\\u0438\\u0447\\u0430\\xA0\\u0442\\u0438.\\u0412\\u044A\\u043F\\u0440\\u0435\\u043A\\u0438 \\u0444\\u0430\\u043A\\u0442\\u0430, \\u0447\\u0435 \\u043C\\u043E\\u0436\\u0435 \\u0434\\u0430 \\u0438\\u0437\\u0433\\u043B\\u0435\\u0436\\u0434\\u0430 \\u0442\\u0430\\u043A\\u0430, \\u0441\\u044F\\u043A\\u0430\\u0448 \\u0431\\u0438\\u0432\\u0448\\u0438\\u044F\\u0442 \\u0432\\u0438 \\u0435 \\u043D\\u0430\\u0434\\u0445\\u0432\\u044A\\u0440\\u043B\\u0438\\u043B\\u0432\\u0440\\u044A\\u0437\\u043A\\u0430, \\u043C\\u043E\\u0436\\u0435 \\u0431\\u0438 \\u0442\\u043E\\u0439 \\u0438\\u043B\\u0438 \\u0442\\u044F \\u043F\\u0440\\u043E\\u0441\\u0442\\u043E \\u0441\\u0435 \\u043E\\u043F\\u0438\\u0442\\u0432\\u0430 \\u0434\\u0430 \\u043F\\u0440\\u0438\\u043A\\u0440\\u0438\\u0435 \\u0442\\u0435\\u0445\\u043D\\u0438\\u0442\\u0435\\xA0\\u0438\\u0441\\u0442\\u0438\\u043D\\u0441\\u043A\\u0438 \\u0447\\u0443\\u0432\\u0441\\u0442\\u0432\\u0430. \\u041F\\u043E-\\u0447\\u0435\\u0441\\u0442\\u043E \\u0435 \\u0441\\u0438\\u0442\\u0443\\u0430\\u0446\\u0438\\u044F\\u0442\\u0430 \\u0441\\u