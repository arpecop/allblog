430 \\u043E\\u0437\\u043D\\u0430\\u0447\\u0430\\u0432\\u0430, \\u0447\\u0435 \\u0442\\u043E\\u0432\\u0430\\u041D\\u0435 \\u043C\\u043E\\u0436\\u0435\\u0445 \\u0434\\u0430 \\u0438\\u043C\\u0430\\u043C \\u0436\\u0435\\u043B\\u0430\\u043D\\u0438\\u0435, \\u043E\\u0441\\u0432\\u0435\\u043D \\u0430\\u043A\\u043E \\u0444\\u0438\\u0437\\u0438\\u0447\\u0435\\u0441\\u043A\\u0438\\u044F\\u0442 \\u043C\\u0443 \\u0435\\u043A\\u0432\\u0438\\u0432\\u0430\\u043B\\u0435\\u043D\\u0442 \\u0432\\u0435\\u0447\\u0435 \\u0431\\u0435\\u0448\\u0435 \\u0442\\u0443\\u043A!\\u0418 \\u043D\\u0435 \\u0441\\u0430\\u043C\\u043E, \\u0447\\u0435 \\u0441\\u0430 \\u0441\\u044A\\u0437\\u0434\\u0430\\u0434\\u0435\\u043D\\u0438 \\u0437\\u0430\\u0435\\u0434\\u043D\\u043E, \\u043D\\u043E \\u0441\\u0430 \\u0441\\u044A\\u0437\\u0434\\u0430\\u0434\\u0435\\u043D\\u0438, \\u0437\\u0430 \\u0434\\u0430 \\u043D\\u0430\\u043C\\u0435\\u0440\\u044F\\u0442 \\u0432\\u0441\\u0435\\u043A\\u0438\\u0434\\u0440\\u0443\\u0433. \\u0412\\u0430\\u0448\\u0435\\u0442\\u043E \\u0436\\u0435\\u043B\\u0430\\u043D\\u0438\\u0435 \\u0432\\u0438 \\u0434\\u044A\\u0440\\u043F\\u0430 \\u043A\\u044A\\u043C \\u043F\\u0440\\u043E\\u044F\\u0432\\u043B\\u0435\\u043D\\u0438\\u0435\\u0442\\u043E \\u0438 \\u043F\\u0440\\u043E\\u044F\\u0432\\u043B\\u0435\\u043D\\u0438\\u0435\\u0442\\u043E \\u0435\\u043F\\u0440\\u0438\\u0432\\u043B\\u0435\\u0447\\u0435\\u043D\\u0438 \\u0438 \\u043A\\u044A\\u043C \\u0432\\u0430\\u0441.\\u041F\\u043E\\u043D\\u044F\\u043A\\u043E\\u0433\\u0430 \\u0442\\u043E\\u0432\\u0430 \\u0436\\u0435\\u043B\\u0430\\u043D\\u0438\\u0435, \\u043A\\u043E\\u0435\\u0442\\u043E \\u0438\\u0437\\u043F\\u0438\\u0442\\u0432\\u0430\\u043C\\u0435, \\u0435 \\u0441\\u043B\\u0430\\u0431 \\u043F\\u043B\\u0430\\u043C\\u044A\\u043A, \\u0430 \\u043F\\u043E\\u043D\\u044F\\u043A\\u043E\\u0433\\u0430 \\u0433\\u043E\\u0440\\u0438 \\u0432\\u0438\\u0441\\u043E\\u043A\\u043E\\u0432\\u0435\\u0434\\u043D\\u0430\\u0433\\u0430. \\u0410\\u043A\\u043E \\u043D\\u0435