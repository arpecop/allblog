\u0434\\u0435\\u043B\\u0435\\u0442\\u0435 \\u043B\\u0438\\u0434\\u0435\\u0440\\u0441\\u043A\\u0438\\u0442\\u0435 \\u043A\\u0430\\u0447\\u0435\\u0441\\u0442\\u0432\\u0430, \\u043A\\u043E\\u0438\\u0442\\u043E \\u043F\\u0440\\u0438\\u0442\\u0435\\u0436\\u0430\\u0432\\u0430\\u0442\\u0435. \\u041A\\u0430\\u043A\\u0432\\u0438 \\u0441\\u044A\\u0431\\u0438\\u0442\\u0438\\u044F \\u043E\\u0444\\u043E\\u0440\\u043C\\u0438\\u0445\\u0430 \\u0436\\u0438\\u0432\\u043E\\u0442\\u0430 \\u0438 \\u043B\\u0438\\u0447\\u043D\\u043E\\u0441\\u0442\\u0442\\u0430 \\u0442\\u0438? \\u041A\\u0430\\u043A\\u0432\\u0438 \\u0443\\u043C\\u0435\\u043D\\u0438\\u044F \\u0441\\u0442\\u0435 \\u043F\\u0440\\u0438\\u0434\\u043E\\u0431\\u0438\\u043B\\u0438? \\u041A\\u043E\\u0439 \\u0435 \\u043D\\u0430\\u0439-\\u0434\\u043E\\u0431\\u0440\\u0438\\u044F\\u0442 \\u0442\\u0438 \\u0430\\u0442\\u0440\\u0438\\u0431\\u0443\\u0442, \\u043A\\u043E\\u0439\\u0442\\u043E \\u0442\\u0435 \\u043F\\u0440\\u0430\\u0432\\u0438 \\u0435\\u043A\\u0438\\u043F \\u0438\\u0433\\u0440\\u0430\\u0447? \\u0417\\u0430\\u0434\\u0430\\u0432\\u0430\\u0439\\u043A\\u0438 \\u0441\\u0438 \\u0442\\u0435\\u0437\\u0438 \\u0432\\u044A\\u043F\\u0440\\u043E\\u0441\\u0438, \\u043C\\u043E\\u0436\\u0435\\u0442\\u0435 \\u0434\\u0430 \\u0441\\u0442\\u0430\\u043D\\u0435\\u0442\\u0435 \\u043F\\u043E-\\u043D\\u0430\\u044F\\u0441\\u043D\\u043E \\u043A\\u0430\\u043A \\u043D\\u0430\\u0439-\\u0434\\u043E\\u0431\\u0440\\u0435 \\u0441\\u0435 \\u0432\\u043F\\u0438\\u0441\\u0432\\u0430\\u0442\\u0435 \\u0432 \\u0435\\u043A\\u0438\\u043F.\\u0412\\u044A\\u0440\\u0432\\u0435\\u0442\\u0435 \\u043F\\u0440\\u0438\\u043A\\u0430\\u0437\\u043A\\u0438\\u0442\\u0435 \\u043D\\u0430 \\u0432\\u0430\\u0448\\u0438\\u0442\\u0435 \\u0434\\u0443\\u043C\\u0438. \\u0412 \\u043B\\u0438\\u0447\\u043D\\u043E\\u0442\\u043E \\u0440\\u0430\\u0437\\u0432\\u0438\\u0442\\u0438\\u0435 \\u043D\\u044F\\u043C\\u0430 \\u043D\\u0438\\u0449\\u043E \\u043F\\u043E-\\u043B\\u043E\\u0448\\u043E \\u0437\\u043