5\\u043A\\u0440\\u0430\\u0441\\u043D\\u043E: \\u201E\\u0417\\u0430\\u0431\\u0435\\u043B\\u0435\\u0436\\u0435\\u0442\\u0435\\u043A\\u043E\\u043B\\u043A\\u043E \\u0434\\u043E\\u0431\\u0440\\u0435 \\u0441\\u0435 \\u0447\\u0443\\u0432\\u0441\\u0442\\u0432\\u0430\\u0442\\u0435, \\u0441\\u043B\\u0435\\u0434 \\u043A\\u0430\\u0442\\u043E \\u0441\\u0442\\u0435 \\u043E\\u043A\\u0443\\u0440\\u0430\\u0436\\u0438\\u043B\\u0438 \\u043D\\u044F\\u043A\\u043E\\u0439 \\u0434\\u0440\\u0443\\u0433. \\u041D\\u044F\\u043C\\u0430 \\u0434\\u0440\\u0443\\u0433\\u0430\\u0440\\u0433\\u0443\\u043C\\u0435\\u043D\\u0442 \\u0435 \\u043D\\u0435\\u043E\\u0431\\u0445\\u043E\\u0434\\u0438\\u043C, \\u0437\\u0430 \\u0434\\u0430 \\u043F\\u043E\\u0434\\u0441\\u043A\\u0430\\u0436\\u0435, \\u0447\\u0435 \\u043D\\u0438\\u043A\\u043E\\u0433\\u0430 \\u043D\\u0435 \\u0442\\u0440\\u044F\\u0431\\u0432\\u0430 \\u0434\\u0430 \\u043F\\u0440\\u043E\\u043F\\u0443\\u0441\\u043A\\u0430\\u0442\\u0435\\u0432\\u044A\\u0437\\u043C\\u043E\\u0436\\u043D\\u043E\\u0441\\u0442 \\u0434\\u0430 \\u0441\\u0435 \\u043E\\u043A\\u0443\\u0440\\u0430\\u0436\\u0438. \\u201D, \\u043E\\u043F\\u0438\\u0441\\u0430 \\u0420\\u0430\\u043B\\u0444 \\u0423\\u043E\\u043B\\u0434\\u043E \\u0415\\u043C\\u0435\\u0440\\u0441\\u044A\\u043D\\u043F\\u043E\\u043C\\u0430\\u0433\\u0430\\u0439\\u043A\\u0438 \\u043D\\u0430 \\u0434\\u0440\\u0443\\u0433\\u0438\\u0442\\u0435 \\u043F\\u043E \\u0442\\u043E\\u0437\\u0438 \\u043D\\u0430\\u0447\\u0438\\u043D: \\u201E\\u041D\\u0435 \\u043C\\u043E\\u0436\\u0435\\u0448 \\u0438\\u0441\\u043A\\u0440\\u0435\\u043D\\u043E \\u0434\\u0430 \\u043F\\u043E\\u043C\\u043E\\u0433\\u043D\\u0435\\u0448 \\u043D\\u0430 \\u0434\\u0440\\u0443\\u0433 \\u0431\\u0435\\u0437\\u043F\\u043E\\u043C\\u0430\\u0433\\u0430\\u0439\\u043A\\u0438 \\u0441\\u0438. \\u201C\\u0418 \\u0442\\u043E\\u0439 \\u043D\\u0435 \\u043C\\u043E\\u0436\\u0435\\u0448\\u0435 \\u0434\\u0430 \\u0435 \\u043F\\u043E-\\u043F\\u0440\\u0430\\u0432. \\u041A\\u043E\\u0433\\u0430\\u