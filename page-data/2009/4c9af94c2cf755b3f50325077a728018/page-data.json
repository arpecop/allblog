411\\u0430\\u0432\\u043D\\u043E \\u0432\\u0434\\u0438\\u0448\\u0430\\u0439\\u0442\\u0435 \\u0438 \\u0432\\u0434\\u0438\\u0433\\u043D\\u0435\\u0442\\u0435 \\u0440\\u044A\\u0446\\u0435 \\u043D\\u0430\\u0434 \\u0433\\u043B\\u0430\\u0432\\u0430\\u0442\\u0430 \\u0441\\u0438.14. \\u041F\\u043E\\u0432\\u0442\\u043E\\u0440\\u0435\\u0442\\u0435 \\u0441\\u0442\\u044A\\u043F\\u043A\\u0438 7 \\u0434\\u043E 13 \\u0434\\u0432\\u0430 \\u043F\\u044A\\u0442\\u0438.15. \\u0417\\u0430\\u0432\\u044A\\u0440\\u0448\\u0435\\u0442\\u0435 \\u043F\\u0440\\u0430\\u043A\\u0442\\u0438\\u043A\\u0430\\u0442\\u0430 \\u0441\\u0438, \\u043A\\u0430\\u0442\\u043E \\u0438\\u0437\\u0434\\u0438\\u0448\\u0430\\u0442\\u0435 \\u043D\\u0435\\u0432\\u043E\\u043B\\u043D\\u043E, \\u0434\\u043E\\u0432\\u0435\\u0436\\u0434\\u0430\\u0439\\u043A\\u0438 \\u0432\\u0430\\u0448\\u0430\\u0442\\u0430\\u0440\\u044A\\u0446\\u0435\\u0442\\u0435 \\u043D\\u0430\\u0434\\u043E\\u043B\\u0443 \\u0438 \\u0440\\u044A\\u0446\\u0435\\u0442\\u0435 \\u0432\\u0438 \\u0437\\u0430\\u0435\\u0434\\u043D\\u043E \\u043F\\u0440\\u0435\\u0434 \\u0433\\u044A\\u0440\\u0434\\u0438\\u0442\\u0435, \\u0434\\u043B\\u0430\\u043D\\u0438\\u0442\\u0435\\u0442\\u0440\\u043E\\u0433\\u0430\\u0442\\u0435\\u043B\\u043D\\u043E.16. \\u041A\\u0430\\u0436\\u0435\\u0442\\u0435 \\u201C\\u0431\\u043B\\u0430\\u0433\\u043E\\u0434\\u0430\\u0440\\u044F\\u201D \\u043D\\u0430 \\u0433\\u043B\\u0430\\u0441 \\u0435\\u0434\\u0438\\u043D \\u043D\\u0430 \\u0434\\u0440\\u0443\\u0433.\\u041A\\u043E\\u043C\\u0435\\u043D\\u0442\\u0430\\u0440\\u0438:\\u041F\\u043E\\u0441\\u0442\\u0430\\u0440\\u0430\\u0439\\u0442\\u0435 \\u0441\\u0435 \\u0434\\u0430 \\u0441\\u0435 \\u0434\\u0432\\u0438\\u0436\\u0438\\u0442\\u0435 \\u043C\\u043D\\u043E\\u0433\\u043E \\u0431\\u0430\\u0432\\u043D\\u043E \\u043F\\u0440\\u0435\\u0437 \\u0442\\u043E\\u0432\\u0430 \\u0443\\u043F\\u0440\\u0430\\u0436\\u043D\\u0435\\u043D\\u0438\\u0435 \\u0441 \\u0438\\u043D\\u0442\\u0435\\u043D\\u0437\\u0438\\u0432\\u043D\\u0