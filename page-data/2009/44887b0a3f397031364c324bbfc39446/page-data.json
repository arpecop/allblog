5\\u044F\\u0442.\\u0412\\u0441\\u044A\\u0449\\u043D\\u043E\\u0441\\u0442 \\u0438\\u043C\\u0430 \\u0424\\u043E\\u043D\\u0434\\u0430\\u0446\\u0438\\u044F \\u201E\\u041F\\u043B\\u0430\\u0442\\u0438 \\u043D\\u0430\\u043F\\u0440\\u0435\\u0434\\u201D, \\u043A\\u043E\\u044F\\u0442\\u043E \\u043F\\u0440\\u0430\\u0432\\u0438 \\u0442\\u043E\\u0447\\u043D\\u043E \\u0442\\u043E\\u0432\\u0430. \\u0422\\u0435 \\u043F\\u043E\\u043A\\u0430\\u0437\\u0432\\u0430\\u0442\\u0434\\u0435\\u0446\\u0430, \\u0440\\u043E\\u0434\\u0438\\u0442\\u0435\\u043B\\u0438 \\u0438 \\u0443\\u0447\\u0438\\u0442\\u0435\\u043B\\u0438 \\u0437\\u0430 \\u043A\\u0430\\u043A\\u0432\\u043E \\u0441\\u0442\\u0430\\u0432\\u0430 \\u0432\\u044A\\u043F\\u0440\\u043E\\u0441. \\u041A\\u0430\\u043A\\u044A\\u0432 \\u043F\\u0440\\u0435\\u043A\\u0440\\u0430\\u0441\\u0435\\u043D \\u043D\\u0430\\u0447\\u0438\\u043D\\u0432\\u0434\\u044A\\u0445\\u043D\\u043E\\u0432\\u0435\\u0442\\u0435 \\u0441\\u043B\\u0435\\u0434\\u0432\\u0430\\u0449\\u043E\\u0442\\u043E \\u043D\\u0438 \\u043F\\u043E\\u043A\\u043E\\u043B\\u0435\\u043D\\u0438\\u0435!\\u0427\\u0435\\u0441\\u0442\\u043E \\u0441\\u044A\\u043C \\u0431\\u0438\\u043B \\u0432 \\u043F\\u0440\\u0438\\u0435\\u043C\\u043D\\u0438\\u044F \\u043A\\u0440\\u0430\\u0439 \\u043D\\u0430 \\u043D\\u044F\\u043A\\u043E\\u0439, \\u043A\\u043E\\u0439\\u0442\\u043E \\u0441\\u0435 \\u0435 \\u0433\\u0440\\u0438\\u0436\\u0438\\u043B \\u0434\\u043E\\u0441\\u0442\\u0430\\u0442\\u044A\\u0447\\u043D\\u043E\\u043D\\u0435\\u0449\\u043E \\u043D\\u0430\\u0438\\u0441\\u0442\\u0438\\u043D\\u0430 \\u043F\\u0440\\u0438\\u044F\\u0442\\u043D\\u043E \\u0437\\u0430 \\u043C\\u0435\\u043D, \\u0431\\u0435\\u0437 \\u0434\\u0430 \\u0441\\u044A\\u043C \\u043F\\u0438\\u0442\\u0430\\u043D \\u0438\\u043B\\u0438 \\u0434\\u043E\\u0440\\u0438 \\u0434\\u0430 \\u0437\\u043D\\u0430\\u043C \\u043A\\u043E\\u0439 \\u0441\\u044A\\u043C. \\u0418\\u0412\\u0440\\u044A\\u0449\\u0430\\u043C \\u0443\\u0441\\u043B\\u0443\\u0433\\u0430\\u0442\\u0430