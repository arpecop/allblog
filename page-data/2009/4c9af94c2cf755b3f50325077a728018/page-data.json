3\\u043E\\u0442\\u043E\\u0432\\u0438 \\u0432 \\u0441\\u0438\\u043D\\u0445\\u0440\\u043E\\u043D \\u0441 \\u043F\\u0430\\u0440\\u0442\\u043D\\u044C\\u043E\\u0440, \\u0442\\u0435 \\u0441\\u044A\\u0449\\u043E\\u0443\\u043A\\u0440\\u0435\\u043F\\u0435\\u0442\\u0435 \\u0441\\u044A\\u0440\\u0434\\u0435\\u0447\\u043D\\u0430\\u0442\\u0430 \\u0441\\u0438 \\u0432\\u0440\\u044A\\u0437\\u043A\\u0430.\\u0412\\u0440\\u0435\\u043C\\u0435 \\u0437\\u0430 \\u0443\\u043F\\u0440\\u0430\\u0436\\u043D\\u0435\\u043D\\u0438\\u044F: \\u0442\\u0440\\u0438 \\u043C\\u0438\\u043D\\u0443\\u0442\\u0438 - \\u043F\\u043E-\\u0434\\u044A\\u043B\\u0433\\u043E \\u0437\\u0430 \\u043F\\u044A\\u0440\\u0432\\u0438 \\u043F\\u044A\\u0442, \\u043A\\u043E\\u0433\\u0430\\u0442\\u043E \\u0433\\u043E \\u043F\\u0440\\u0430\\u0432\\u0438\\u0442\\u0435\\u041D\\u0435\\u043E\\u0431\\u0445\\u043E\\u0434\\u0438\\u043C\\u0438 \\u0441\\u0432\\u043E\\u0439\\u0441\\u0442\\u0432\\u0430: \\u043E\\u0431\\u043B\\u0435\\u043A\\u043B\\u043E, \\u0432 \\u043A\\u043E\\u0435\\u0442\\u043E \\u043C\\u043E\\u0436\\u0435\\u0442\\u0435 \\u0434\\u0430 \\u0441\\u0435 \\u0440\\u0430\\u0437\\u0442\\u0435\\u0433\\u043D\\u0435\\u0442\\u0435 \\u0443\\u0434\\u043E\\u0431\\u043D\\u043E\\u0441\\u0442\\u044A\\u043F\\u043A\\u0438:1. \\u0417\\u0430\\u0441\\u0442\\u0430\\u043D\\u0435\\u0442\\u0435 \\u0435\\u0434\\u0438\\u043D \\u0441\\u0440\\u0435\\u0449\\u0443 \\u0434\\u0440\\u0443\\u0433 \\u043D\\u0430 \\u0440\\u0430\\u0437\\u0441\\u0442\\u043E\\u044F\\u043D\\u0438\\u0435 \\u043E\\u043A\\u043E\\u043B\\u043E 4 \\u0444\\u0443\\u0442\\u0430.2. \\u041A\\u0440\\u0430\\u043A\\u0430\\u0442\\u0430 \\u0432\\u0438 \\u0441\\u0430 \\u0437\\u0430\\u0435\\u0434\\u043D\\u043E, \\u0440\\u044A\\u0446\\u0435\\u0442\\u0435 \\u0432\\u0438\\u0441\\u044F\\u0442 \\u043B\\u0435\\u043A\\u043E \\u043E\\u0442\\u0441\\u0442\\u0440\\u0430\\u043D\\u0438, \\u043E\\u0442\\u0437\\u0430\\u0434\\u043F\\u0440\\u0430\\u0432\\u0438, \\u043D\\u043E \\u043D\\u0435 \\u0442\\u0432\\u044A\\u04